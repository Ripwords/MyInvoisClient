{
  "name": "@ripwords/myinvois-client",
  "version": "0.0.10",
  "description": "MyInvois client",
  "scripts": {
    "build": "bun run clean && rolldown -c rolldown.config.ts",
    "release": "bun run build && npm publish --access public",
    "clean": "rimraf dist",
    "lint": "oxlint",
    "test": "vitest"
  },
  "main": "dist/index.js",
  "module": "src/index.ts",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@vitest/coverage-v8": "^3.1.2",
    "dotenv": "^16.5.0",
    "oxlint": "^0.16.6",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rolldown": "^1.0.0-beta.7",
    "rolldown-plugin-dts": "^0.7.13",
    "vitest": "^3.1.1",
    "@types/xml-c14n": "^0.0.3"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.9.8",
    "xml-c14n": "^0.0.6",
    "xml-crypto": "^6.1.2",
    "xmldom-ts": "^0.3.1",
    "xpath-ts": "^1.3.13"
  }
}